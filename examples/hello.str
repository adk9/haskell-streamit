/*
 * HelloWorld.str: a serial integer printer
 * $Id: HelloWorld.str,v 1.1 2006/09/08 17:18:29 thies Exp $
 */

void->int filter IntSource {
    int x;
    init {
        x = 0;
    }
    work push 1 {
        push(x++);
    }
}

int->void filter IntPrinter {
    work pop 1 {
        println(pop());
    }
}

void->void pipeline HelloWorld {
    add IntSource();
    add IntPrinter();
}
