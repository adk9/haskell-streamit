void->int filter SortInput(int N)
{
	work push N {
		int i = 0;
		for (i = 0; (i < N); i = (i + 1)) {
			push((N - i));
		}
	}
}

int->void filter IntPrinter()
{
	work pop 1 {
		println(pop());
	}
}

void->void pipeline MergeSort {
	int NUM_INPUTS = 16;
	int MULT = 4;
	add SortInput((NUM_INPUTS / MULT));
	add IntPrinter();
}

